{
  "name": "10x-marketing-team",
  "version": "2.0.0",
  "description": "AI-powered marketing video suite with Remotion Studio and Agentation integration",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "npm install && npx remotion install ffmpeg && npx remotion install ffprobe",
    "remotion:studio": "remotion studio src/remotion/index.ts",
    "remotion:preview": "remotion preview src/remotion/index.ts",
    "remotion:render": "remotion render src/remotion/index.ts",
    "remotion:render:local": "remotion render src/remotion/index.ts --codec=h264",
    "remotion:build": "remotion bundle src/remotion/index.ts --out-dir=remotion-bundle",
    "remotion:upgrade": "npx remotion upgrade",
    "remotion:install-deps": "npx remotion install ffmpeg && npx remotion install ffprobe"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "agentation": "latest",
    "remotion": "^4.0.0",
    "@remotion/cli": "^4.0.0",
    "@remotion/player": "^4.0.0",
    "@remotion/renderer": "^4.0.0",
    "@remotion/bundler": "^4.0.0",
    "@remotion/transitions": "^4.0.0",
    "@remotion/shapes": "^4.0.0",
    "@remotion/paths": "^4.0.0",
    "@remotion/gif": "^4.0.0",
    "@remotion/noise": "^4.0.0",
    "@remotion/motion-blur": "^4.0.0",
    "@remotion/media-utils": "^4.0.0",
    "@remotion/preload": "^4.0.0",
    "@remotion/three": "^4.0.0",
    "@remotion/lottie": "^4.0.0",
    "@remotion/rive": "^4.0.0",
    "@remotion/lambda": "^4.0.0",
    "@remotion/cloudrun": "^4.0.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "clsx": "^2.1.0",
    "zod": "^3.23.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.160.0",
    "typescript": "^5.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0"
  }
}
